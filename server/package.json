{
  "name": "gameserver",
  "version": "0.0.1",
  "description": "Server for handling API calls",
  "main": "index.ts",
  "scripts": {
    "start": "node index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec \"node --inspect -r ts-node/register index.ts\"",
    "debug": "node --inspect index.ts"
  },
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "test/*",
      "docs/*"
    ],
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts,js,json",
    "delay": 100
  },
  "dependencies": {
    "eslint": "^7.32.0",
    "typescript": "^4.1.2",
    "prettier": "^2.3.2",
    "express": "^4.17.3",
    "ts-node": "^10.7.0"
  }
}
