{
  "name": "test",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "startServ": "nodemon --exec \"node --inspect -r ts-node/register src/server.ts\"",
    "buildServ": "tsc",

    "startCli": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",

    "start": "concurrently \"yarn startServ\" \"yarn startCli\""
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "test/*",
      "docs/*"
    ],
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts,js,tsx,json",
    "delay": 100
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "eslint": "^7.32.0",
    "express": "^4.17.3",
    "json-loader": "^0.5.7",
    "next": "^11.0.1",
    "prettier": "^2.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "swr": "^0.5.6",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "17.0.43",
    "concurrently": "^7.0.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "nodemon": "^2.0.15",
    "typescript": "4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
